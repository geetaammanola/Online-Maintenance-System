 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	
	
	<servlet>
		<servlet-name>storeDataController</servlet-name>
		<display-name>storeDataController</display-name>
		<description></description>
		<servlet-class>com.serv.web.controller.storeDataController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>logoutSer</servlet-name>
		<display-name>logoutSer</display-name>
		<description></description>
		<servlet-class>com.serv.web.controller.logoutSer</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>storeDataController</servlet-name>
		<url-pattern>/storeData</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>logoutSer</servlet-name>
		<url-pattern>/logoutSer</url-pattern>
	</servlet-mapping>
	
	
